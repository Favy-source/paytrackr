{
  "expo": {
    "name": "PayTrackr",
    "slug": "paytrackr",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "scheme": "paytrackr",
    "owner": "favy-source",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#6200EE"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.paytrackr.app"
    },
    "android": {
      "package": "com.paytrackr.app",
      "permissions": ["POST_NOTIFICATIONS"],
      "useNextNotificationsApi": true
    },
     "plugins": [
      "expo-notifications"
    ],
    "extra": {
      "eas": {
        "projectId": "01f4ce84-d573-4a48-b334-80d287f4b33b"
      },
      "apiUrl": "https://natural-gentleness-production.up.railway.app/api"
    }
  }
}
